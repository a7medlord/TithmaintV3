@model CloudApp.Models.BusinessModel.Treatment      

<div class="tab-pane  active" id="tab2-1">

    <div asp-validation-summary="ModelOnly" class="text-danger"></div>

    <div class="row">

        <div class="col-md-3" style="pointer-events: none">
            <div class="form-group">
                <label asp-for="CustmerId" class="col-md-2 control-label"></label>

                <select asp-for="CustmerId" class="select2 form-control">
                    <option value="@ViewBag.cmsname.Id">@ViewBag.cmsname.Name</option>
                </select>

            </div>
        </div>
        
        <div class="col-md-3">
            <div class="form-group">
                <label asp-for="Scustmer" class="control-label"></label>
                <input asp-for="Scustmer" class="form-control" />
